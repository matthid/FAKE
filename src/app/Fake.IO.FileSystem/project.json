{
  "version": "1.0.0-alpha7",
  "buildOptions": {
    "debugType": "portable",
    "compilerName": "fsc",
    "define": [
      "DOTNETCORE"
    ],
    "emitEntryPoint": false,
    "xmlDoc": true,
    "compile": {
      "includeFiles": [
        "AssemblyInfo.fs",
        "Path.fs",
        "FileSystem.fs",
        "FileUtils.fs"
      ]
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    }
  },
  "dependencies": {
    "FSharp.Core": "4.0.1.7-alpha",
    "Fake.Core.String": "1.0.0-alpha7"
  },
  "frameworks": {
    "net46": {
      "dependencies": {
        "FSharp.Compiler.Service": "3.0.0"
      },
      "frameworkAssemblies": {
        "Microsoft.Build.Framework": "",
        "mscorlib": "",
        "System": "",
        "System.Configuration": "",
        "System.Runtime.Caching": "",
        "System.ServiceProcess": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "System.Core": ""
      }
    },
    "netstandard1.6": {
      "buildOptions": {
        "define": [
          "NETSTANDARD1_5"
        ]
      },
      "imports": [
        "portable-net45+win8",
        "dnxcore50"
      ],
      "dependencies": {
        "System.Diagnostics.FileVersionInfo": "4.0.0",
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}