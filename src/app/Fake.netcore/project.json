{
  "version": "1.0.0-alpha1",
  "copyright": "Appparently dotnet cli needs this.",
  "buildOptions": {
    "compilerName": "fsc",
    "define": [
      "CORE_CLR"
    ],
    "emitEntryPoint": true,
    "xmlDoc": true,
    "delaySign": true,
    "additionalArguments": [
      "--warn:3",
      "--fullpaths",
      "--flaterrors",
      "--warnon:1182"
    ],
    "compile": {
      "includeFiles": [
        "Cli.fs",
        "Program.fs"
      ]
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview1-*",
      "imports": [
        "dnxcore50",
        "portable-net45+win81",
        "netstandard1.3"
      ]
    }
  },
  "frameworks": {
    "net46": {
      "dependencies": {
        "FSharp.Core": "4.0.0.1",
        "FSharp.Compiler.Service": "3.0.0",
        "Mono.Cecil": "0.9.6.1",
        "FakeLib.netcore": "1.0.0-alpha1",
        "CommandLineParser": "2.0.275-beta"
      },
      "frameworkAssemblies": {
        "Microsoft.Build.Framework": "",
        "mscorlib": "",
        "System": "",
        "System.Configuration": "",
        "System.Runtime.Caching": "",
        "System.ServiceProcess": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "System.Core": ""
      }
    },
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [
          "NETSTANDARD1_5"
        ]
      },
      "imports": [
        "portable-net45+win8",
        "dnxcore50"
      ],
      "dependencies": {
        "Mono.Cecil": "0.9.6.0",
        "FakeLib.netcore": "1.0.0-alpha3",
        "CommandLine.DotNetCore": "1.0.0",
        "System.Console": "4.0.0-rc2-24027",
        "System.IO.FileSystem": "4.0.1",
        "System.Diagnostics.Tracing": "4.1.0",
        "System.Net.NameResolution": "4.0.0",
        "System.Net.Primitives": "4.0.11",
        "System.Threading.ThreadPool": "4.0.10",
        "System.Diagnostics.TraceSource": "4.0.0",
        "System.Reflection": "4.1.0",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Reflection.Primitives": "4.0.1",
        "System.Runtime": "4.1.0",
        "System.Runtime.Loader": "4.0.0",
        "System.Runtime.Extensions": "4.1.0",
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*",
        "FSharp.Compiler.Service.netcore": "1.0.0-alpha-00001",
        "System.Security.Cryptography.Primitives": "4.0.0",
        "System.Security.Cryptography.Algorithms": "4.2.0",
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0-rc2-*"
        }
      }
    }
  }
}