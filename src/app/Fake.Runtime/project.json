        "LegacyApiHelper.fs",
        "Environment.fs",
        "Path.fs",
        "String.fs",
        "Trace.fs",
        "ScriptRunner.fs",
        "HashGeneration.fs",
        "CoreCache.fs",
        "FakeRuntime.fs"
      ]
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    }
  },
  "dependencies": {
    "Paket.Core.netcore": "3.4.0-alpha002"
  },
  "frameworks": {
    "net46": {
      "dependencies": {
        "FSharp.Core": "4.0.0.1",
        "FSharp.Compiler.Service": "3.0.0",
        "Mono.Cecil": "0.9.6.1"
      },
      "frameworkAssemblies": {
        "Microsoft.Build.Framework": "",
        "mscorlib": "",
        "System": "",
        "System.Configuration": "",
        "System.Runtime.Caching": "",
        "System.ServiceProcess": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "System.Core": ""
      }
    },
    "netstandard1.6": {
      "buildOptions": {
        "define": [
          "NETSTANDARD1_5"
        ]
      },
      "imports": [
        "portable-net45+win8",
        "dnxcore50"
      ],
      "dependencies": {
        "Mono.Cecil": "0.9.6.0",
        "System.Diagnostics.TraceSource": "4.0.0",
        "System.Reflection": "4.1.0",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Reflection.Primitives": "4.0.1",
        "System.Runtime": "4.1.0",
        "System.Runtime.Loader": "4.0.0",
        "System.Runtime.Extensions": "4.1.0",
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160629",
