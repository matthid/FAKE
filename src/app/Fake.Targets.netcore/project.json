{
  "version": "1.0.0-alpha1",
  "buildOptions": {
    "debugType": "portable",
    "compilerName": "fsc",
    "define": [
      "CORE_CLR",
      "EXPLICIT_DEPENDENCIES"
    ],
    "emitEntryPoint": false,
    "xmlDoc": true,
    "compile": {
      "includeFiles": [
        "../FakeLib/AssemblyInfo.fs",
        "../FakeLib/TargetHelper.fs",
        "../FakeLib/AdditionalSyntax.fs"
      ]
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    }
  },
  "dependencies": {
    "FakeLib.netcore": "1.0.0-alpha4"
  },
  "frameworks": {
    "net46": {
      "dependencies": {
        "FSharp.Core": "4.0.0.1",
        "FSharp.Compiler.Service": "3.0.0"
      },
      "frameworkAssemblies": {
        "Microsoft.Build.Framework": "",
        "mscorlib": "",
        "System": "",
        "System.Configuration": "",
        "System.Runtime.Caching": "",
        "System.ServiceProcess": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "System.Core": ""
      }
    },
    "netstandard1.6": {
      "buildOptions": {
        "define": [
          "NETSTANDARD1_5"
        ]
      },
      "imports": [
        "portable-net45+win8",
        "dnxcore50"
      ],
      "dependencies": {
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160629",
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}